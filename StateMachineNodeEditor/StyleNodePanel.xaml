<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:StateMachineNodeEditor">
    <TransformGroup x:Key="Transform1" >
        <ScaleTransform  ScaleX="{Binding ElementName=ScaleX, Path=ScaleX}" ScaleY="{Binding ElementName=ScaleY, Path=ScaleY}"/>
        <SkewTransform/>
        <RotateTransform/>
        <TranslateTransform X="{Binding ElementName=TransformX, Path=TransformX}" Y="{Binding ElementName=TransformY, Path=TransformY}" />
    </TransformGroup>
</ResourceDictionary>